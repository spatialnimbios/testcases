# Input file

start parameters
    nItr = 1000000000
    timeStep = 0.02
   #    pdbWrite = 1000
    timeWrite = 500
    trajWrite = 100000
    restartWrite = 100000
    checkPoint = 10000000
end parameters

start boundaries
    waterBox = [144.22,144.22,144.22] #in nm
end boundaries

start molecules
    A : 103 (ser~U), 5 (ser~P)
    Phos : 9
end molecules

start reactions
    # Rxn Nums: 0,1. MICHAELIS MENTEN#
    A(ser~P) + A(ser~U) <-> A(ser~P!1).A(ser~U!1)
    onRate3Dka = 16.7   # nm3/us
    offRatekb =0.0 #s-1
    kcat = 200.0 #s-1
    sigma = 1
    coupledRxnLabel = autoP 
    norm1=[0,0,1]
    norm2=[0,0,1]
    assocAngles=[nan, nan, nan, nan, nan]

    #Rxn Num: 2,3, MICHAELIS MENTEN
    A(ser~P) + Phos(de) <-> A(ser~P!1).Phos(de!1)
    onRate3Dka = 2820.0  # nm3/us
    offRatekb = 0.0 #s-1
    kcat = 539.0 #s-1
    sigma = 1
    coupledRxnLabel = autoDP 
    norm1=[0,0,1]
    norm2=[0,0,1]
    assocAngles=[nan, nan, nan, nan, nan]

    #Rxn Num: 4, autoPhosphorylation
    A(ser~U) -> A(ser~P)
    rate = 21.2   # s-1
    rxnLabel = autoP

    #Rxn Num: 5, auto-DEphosphorylation
    A(ser~P) -> A(ser~U)
    rate = 0   # s-1. This never happens spontaneously
    rxnLabel = autoDP

end reactions
